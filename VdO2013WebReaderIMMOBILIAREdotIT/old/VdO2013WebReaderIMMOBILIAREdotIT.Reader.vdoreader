        <!--Each FeatureElement defines the following properties:
					Name:name	Type:System.String	Attributes:name{DefaultValue="";IsRequired=(Boolean)True;IsKey=(Boolean)True;}
					Name:xPath	Type:System.String	Attributes:xPath{IsRequired=(Boolean)True;DefaultValue="";}
					Name:mapping	Type:System.String	Attributes:mapping{IsRequired=(Boolean)True;DefaultValue="";}
					Name:typeName	Type:System.String	Attributes:typeName{IsRequired=(Boolean)True;DefaultValue="System.String";}
					Name:maxSize	Type:System.Int32	Attributes:maxSize{IsRequired=(Boolean)True;DefaultValue=(Int32)-1;}
					Name:minSize	Type:System.Int32	Attributes:minSize{IsRequired=(Boolean)False;DefaultValue=(Int32)-1;}
					Name:nullable	Type:System.Boolean	Attributes:nullable{IsRequired=(Boolean)True;DefaultValue=(Boolean)True;}
					Name:actions	Type:VdO2013Main.Config.FeatureElementActions	Attributes:actions{IsRequired=(Boolean)False;}

Each FeatureElementActions defines the following properties:
					Name:isNull	Type:System.String	Attributes:isNull{IsRequired=(Boolean)False;DefaultValue="";}
					Name:nullIf	Type:System.String	Attributes:nullIf{IsRequired=(Boolean)False;DefaultValue="";}
					Name:trimLeft	Type:System.Boolean	Attributes:trimLeft{IsRequired=(Boolean)False;DefaultValue=(Boolean)False;}
